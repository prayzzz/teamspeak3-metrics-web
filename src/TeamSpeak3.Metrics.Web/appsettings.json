{
  "App": {
    "TS3Server": {
      "Host": "127.0.0.1",
      "Port": 9987,
      "QueryPort": 10011,
      "QueryUsername": "",
      "QueryPassword": ""
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "TeamSpeak3.Metrics.Web"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "OutputTemplate": "{Timestamp:HH:mm:ss} {Level:u3} [{SourceContext}]: {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}